{
  "name": "store-operations-ui",
  "description": "vue 组件库",
  "version": "0.2.3",
  "type": "module",
  "main": "./dist/index.d.ts",
  "module": "./dist/store-operations-ui.es.js",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "dev": "vitepress dev docs",
    "build": "vite build && vue-tsc",
    "preview": "vite preview",
    "format": "prettier --write \"./**/*.{html,vue,ts,js,json,md}\"",
    "lint": "npm run tsc && npm run lint:demo && npm run lint:md && npm run lint:script && npm run lint:site",
    "lint:components": "eslint --fix --ext .jsx,.js,.ts,.tsx ./components",
    "lint:script": "eslint . --ext '.js,.jsx,.ts,.tsx'",
    "lint:site": "eslint -c ./.eslintrc.js --fix --ext .jsx,.js,.ts,.tsx,vue ./site",
    "lint:style": "stylelint \"{site,components}/**/*.scss\" --syntax scss",
    "pub": "yarn publish --registry=https://registry.npmjs.org/.",
    "prepare": "husky install",
    "prettier": "prettier -c --write **/*",
    "myLint": "./node_modules/.bin/eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "release": "release-it",
    "commit": "git-cz"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{css,scss,less}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vueuse/core": "^10.3.0",
    "ant-design-vue": "^4.0.2",
    "autoprefixer": "^10.4.15",
    "babel-eslint": "^10.1.0",
    "child_process": "^1.0.2",
    "eslint-plugin-import": "^2.28.0",
    "path": "^0.12.7",
    "postcss": "^8.4.28",
    "prettier": "^3.0.0",
    "sass": "^1.64.1",
    "url": "^0.11.1",
    "vue": "^3.3.4",
    "vue-hooks-plus": "^1.8.2",
    "vue-tsc": "^1.8.5",
    "wa-utils": "0.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.0",
    "@commitlint/config-conventional": "^12.0.0",
    "@release-it/conventional-changelog": "^7.0.0",
    "@types/node": "^20.4.5",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "eslint": "^8.46.0",
    "eslint-plugin-vue": "^9.16.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.0.3",
    "release-it": "^16.1.3",
    "stylelint": "^15.10.2",
    "stylelint-config-standard-scss": "^10.0.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.7",
    "vite-svg-loader": "^4.0.0",
    "vitepress": "^1.0.0-beta.7",
    "wa-postcss-pxtorem": "^0.0.1"
  },
  "resolutions": {
    "@typescript-eslint/typescript-estree": "^5.59.2"
  }
}
